citHeader("To cite seqpac in publications use:")

citEntry(entry = "Article",
  title        = "Seqpac: A New Framework for small RNA analysis in R using Sequence-Based Counts",
  author       = personList(as.person("Signe Skog"),
                            as.person("Lovisa Orkenby"),
			    as.person("Unn Kugelberg"),
			    as.person("Kanwal Tariq"),
			    as.person("Ann-Kristin Ostlund Farrants"),
			    as.person("Anita Ost"),
			    as.person("Daniel Natt")),
  journal      = "bioRxiv",
  year         = "2021",
  doi	       = "10.1101/2021.03.19.436151",
  volume       = "_",
  number       = "_",
  pages        = "_",
  textVersion  =
  paste("Signe Skog, Lovisa Orkenby, Unn Kugelberg, Kanwal Tariq, Ann-Kristin Ostlund Farrants, Anita Ost, Daniel Natt (2021).",
        "Seqpac: A New Framework for small RNA analysis in R using Sequence-Based Counts.",
        "bioRxiv",
        "_")
)
